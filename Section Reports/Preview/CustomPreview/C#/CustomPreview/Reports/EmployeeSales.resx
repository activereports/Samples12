  <data name="$this.Script" xml:space="preserve">
    <value>// counter for the alternating row colors 
private long _greenBarCounter = 0; 

// Handles the event used to manipulate the detail section 
public void Detail_Format()
{
	// Alternates the back color using a counter and an immediate if statement
	rpt.Sections["Detail"].BackColor = (_greenBarCounter++ % 2 == 0)? System.Drawing.Color.Transparent : System.Drawing.Color.LightGray;
}</value>
