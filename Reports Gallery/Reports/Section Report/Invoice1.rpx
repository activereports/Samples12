}public void Detail_Format()
{
	// Increment the line number.
	iRow++;

	// Switch the background color, one per line.
	if ((iRow % 2) == 1)
	{
		this.shpDetailBack.BackColor = Color.White;	// White
	}
	else
	{
		this.shpDetailBack.BackColor = Color.WhiteSmoke ;	//Light Grey
	}
}


public void ActiveReport_DataInitialize()
{
	
	// To create an unbound field to display the price.
	rpt.Fields.Add("Price");

}


public bool ActiveReport_FetchData(bool eof)
{
	
	// Sets the value of the unbound field.
	rpt.Fields["Price"].Value = Convert.ToDecimal((rpt.Fields["Number"].Value))* Convert.ToDecimal((rpt.Fields["UnitPrice"].Value));
	return eof;
}


