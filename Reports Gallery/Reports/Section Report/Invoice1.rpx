public void PageHeader_BeforePrint()
{
	// Refer to the aggregate value, and then set the value to another control Field.
	this.txtExcise.Value = (object) ((double) this.txtTotal.Value * 0.05);	// Consumption tax
	this.txtBillTotal.Value = (object) ((double) this.txtTotal.Value * 1.05 + (double) this.txtCarryOver.Value);	//The total amount charged
