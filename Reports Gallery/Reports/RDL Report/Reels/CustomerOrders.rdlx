				<Field Name="CustomerName">
					<Value>=Fields!FirstName.Value &amp; IIf( IsNothing(Fields!MiddleInitial.Value), "", " " &amp; Fields!MiddleInitial.Value &amp; "." ) &amp; Fields!LastName.Value</Value>
