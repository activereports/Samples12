	<Code>Public Function RemoveAmpersand(obj As Object) As String
	Dim ret As String 
	If TypeOf obj Is String Then
		Dim str As String = obj
		ret = str.Replace("&amp;", "&amp;amp;") 
	Else
		ret =obj.ToString()
	End If
	Return ret 
End Function</Code>
